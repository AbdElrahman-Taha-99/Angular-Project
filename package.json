{
  "name": "angular.io-example",
  "version": "0.0.0",
  "type": "module",
  "description": "Example project from an angular.io guide.",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint": "eslint ./src --ext .ts,.html",
    "lint:fix": "eslint . --ext .ts,.html --fix",
    "format": "prettier --write \"src/**/*.{ts,html,scss,css,json,md}\""
  },
  "lint-staged": {
    "*.{ts,html,scss,css,json,md}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.0-rc.0",
    "@angular/common": "^16.1.0-rc.0",
    "@angular/compiler": "^16.1.0-rc.0",
    "@angular/core": "^16.1.0-rc.0",
    "@angular/forms": "^16.1.0-rc.0",
    "@angular/platform-browser": "^16.1.0-rc.0",
    "@angular/platform-browser-dynamic": "^16.1.0-rc.0",
    "@angular/router": "^16.1.0-rc.0",
    "angular-in-memory-web-api": "~0.16.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.0-rc.0",
    "@angular-eslint/eslint-plugin": "^20.3.0",
    "@angular-eslint/eslint-plugin-template": "^20.3.0",
    "@angular-eslint/template-parser": "^20.3.0",
    "@angular/cli": "^16.1.0-rc.0",
    "@angular/compiler-cli": "^16.1.0-rc.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^16.11.35",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@typescript-eslint/typescript-estree": "^8.44.1",
    "@typescript-eslint/utils": "^8.44.1",
    "copyfiles": "^2.4.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jasmine-core": "~4.6.0",
    "jasmine-marbles": "~0.9.2",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-junit-reporter": "^2.0.1",
    "lint-staged": "^16.2.0",
    "prettier": "^3.6.2",
    "protractor": "~7.0.0",
    "ts-node": "~10.9.0",
    "typescript": "~4.9.3"
  }
}
